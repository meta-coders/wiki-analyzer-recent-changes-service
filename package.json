{
  "name": "wiki-analyzer-recent-changes-service",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.test.json && tap test/**/*.test.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\"  \"tsc -w\" \"fastify start -w -P dist/app.js\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix"
  },
  "repository": "git@github.com:meta-coders/wiki-analyzer-recent-changes-service.git",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meta-coders/wiki-analyzer-recent-changes-service/issues"
  },
  "homepage": "https://github.com/meta-coders/wiki-analyzer-recent-changes-service#readme",
  "dependencies": {
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.0.2",
    "fastify-cli": "^2.6.0",
    "fastify-plugin": "^3.0.0",
    "fastify-websocket": "^2.0.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "ws": "^7.4.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.18",
    "@types/tap": "^14.10.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "concurrently": "^5.1.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "fastify-tsconfig": "^1.0.0",
    "prettier": "^2.2.1",
    "tap": "^14.0.0",
    "typescript": "^4.0.2"
  }
}
